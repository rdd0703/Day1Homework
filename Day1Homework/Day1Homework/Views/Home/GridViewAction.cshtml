@model Day1Homework.Models.MyClassModel
@{
    Layout = null;
 }

<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tr>
                <th>#</th>
                <th>類別</th>
                <th>日期</th>
                <th>金額</th>
            </tr>
            @{
                foreach (Day1Homework.Models.MyClassModel myClass in (List<Day1Homework.Models.MyClassModel>)ViewBag.MyClassModels)
                {
                    <tr>
                        <td>@myClass.Sn</td>
                        <td>@myClass.Category</td>
                        <td>@myClass.Date</td>
                        <td>@myClass.Amount</td>
                    </tr>
                }
            }
        </table>
    </div>
</div>
